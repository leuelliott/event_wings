<?xml version='1.0' encoding='utf-8'?>
<widget id="com.proevent.dele.eventwings" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EventWing</name>
    <description>
        An Event Management System.
    </description>
    <author email="deelesisuanu@gmail.com" href="http://deelesisuanu.com">
        Deelesi Suanu
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="loadUrlTimeoutValue" value="240000" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#7840d8" />
    <preference name="StatusBarStyle" value="lightcontent" />
</widget>
